#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.3.94_postcss@8.4.31_typescript@5.2.2/node_modules/tsup/dist/node_modules:/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.3.94_postcss@8.4.31_typescript@5.2.2/node_modules/tsup/node_modules:/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.3.94_postcss@8.4.31_typescript@5.2.2/node_modules:/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.3.94_postcss@8.4.31_typescript@5.2.2/node_modules/tsup/dist/node_modules:/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.3.94_postcss@8.4.31_typescript@5.2.2/node_modules/tsup/node_modules:/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.3.94_postcss@8.4.31_typescript@5.2.2/node_modules:/Users/bitwhys/Projects/.abittechnical/digital-garden/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
